    "eslint-plugin-jest": "^27.4.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.8.8",
    "ts-jest": "^29.0.3",
    "typescript": "^5.2.2"
