{
  "name": "action-check-pr-title",
  "version": "5.0.1",
  "description": "Github action to check Pull Request title based on JS Regexp",
  "repository": "git@github.com:Slashgear/action-check-pr-title.git",
  "author": "Slashgear <antoine395.caron@gmail.com>",
  "license": "MIT",
  "keywords": [
    "github",
    "actions"
  ],
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "oxlint .",
    "format": "prettier --write --ignore-unknown '**/*'",
    "format-check": "prettier --check --ignore-unknown '**/*'",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "vitest",
    "preversion": "pnpm run build && pnpm run package && git add ."
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "@vercel/ncc": "^0.38.3",
    "js-yaml": "^4.1.0",
    "oxlint": "^1.14.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.16.1+sha512.0e155aa2629db8672b49e8475da6226aa4bdea85fdcdfdc15350874946d4f3c91faaf64cbdc4a5d1ab8002f473d5c3fcedcd197989cf0390f9badd3c04678706"
}
